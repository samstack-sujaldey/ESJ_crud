<h1>USERS</h1>
<a href="/new">Add User</a>

<ul>
    <% users.forEach(user=> { %>
        <li>
            <%=user.name%> - <%=user.email%>
                    <a href="edit/<%=user._id%>">Edit User</a>
                    <form action="/delete/<%=user._id%>?_method=DELETE" method="POST" style="display: inline-block;">
                        <button>Delete</button>
                    </form>
        </li>
        <% }) %>
</ul>